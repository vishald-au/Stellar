{
  "expo": {
    "name": "Stellar",
    "slug": "stellar",
    "description": "Demo app for portfolio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/stellar.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#DA6264"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/stellar.png",
        "backgroundColor": "#DA6264"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/stellar.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2d472102-4c9a-4980-9dff-f50f674e7935"
      }
    },
    "owner": "vishal-au"
  }
}
